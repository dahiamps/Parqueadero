<!DOCTYPE html>
<html>

<head>
    <meta charset='utf-8'>
    <meta http-equiv='X-UA-Compatible' content='IE=edge'>
    <title>Registro de vehiculos</title>
    <meta name='viewport' content='width=device-width, initial-scale=1'>
    <link rel='stylesheet' type='text/css' media='screen' href='main.css'>
    <script src='main.js'></script>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
    <link href='https://unpkg.com/boxicons@2.0.9/css/boxicons.min.css' rel='stylesheet'>
    <link rel="stylesheet" href="https://cdn.datatables.net/1.10.25/css/dataTables.bootstrap5.min.css">

</head>

<body>
    <header>
        <nav class="navbar navbar-expand-lg navbar-light bg-light">
            <div class="container">
                <a class="navbar-brand" href="/"><i class='bx bxs-car-garage'></i></a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNav"
                    aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="collapse navbar-collapse" id="navbarNav">
                    <ul class="navbar-nav">
                        <li class="nav-item">
                            <a class="nav-link " aria-current="page" href="/">Inicio</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="/propietarios">Propietarios</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link active" href="/vehiculos">Vehiculos</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link " href="/parqueadero">Parqueadero</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="/Historial">Historial de salidas</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    <main>


        <div class="container ">
            <div class="row">
                <div class="col-9 col-md-4">
                    <form action="/save" method="POST">
                        <div class="row justify-content-md-center">

                            <div class="col-md-auto">
                                <h1>Registro de Vehiculos</h1>
                            </div>


                        </div>
                        <input type="text" name="Placa" id="Placa" placeholder="Numero de Placa"
                            class="form-control es my-3">
                        <input type="text" name="Marca" id="Marca" placeholder="Marca Vehiculo"
                            class="form-control es my-3">
                        <input type="text" name="Modelo" id="Modelo" placeholder="Modelo Vehiculo"
                            class="form-control es my-3">
                        <input type="text" name="Color" id="Color" placeholder="Color Vehiculo"
                            class="form-control es my-3">
                        <input type="text" name="cedula" id="cedula" placeholder="Cedula propietario"
                            class="form-control es my-3">
                       

                        <div class="row justify-content-md-center">
                            <div class="col-md-auto">
                                <input type="submit" class="btn btn-success btnr" value="Guardar">

                                <a href="/vehiculos" class="btn btn-primary btnr">Cancelar</a>
                            </div>



                        </div>



                    </form>
                </div>
                <div class="col-8">
                    <div class="container">
                        <div class="row justify-content-md-center">

                            <div class="col-md-auto">
                                <h1>Mis Vehiculos</h1>
                            </div>


                        </div>
                        <table id="vehiculo" class="table table bordered table-striped tex-center mt-4">
                            <thead>
                                <tr class="bg-primary text-white">
                                    <th scope="col">Placa</th>
                                    <th scope="col">Marca</th>
                                    <th scope="col">Modelo</th>
                                    <th scope="col">Color</th>
                                    <th scope="col">Cedula Propietario</th>

                                    <!-- <th scope="col">Acciones</th> -->
                                </tr>
                            </thead>
                            <tbody>
                                <% results.forEach((cliente)=> { %>

                                    <tr>
                                        <td>
                                            <%= cliente.Placa %>
                                        </td>
                                        <td>
                                            <%= cliente.Marca %>
                                        </td>
                                        <td>
                                            <%= cliente.Modelo %>
                                        </td>
                                        <td>
                                            <%= cliente.Color %>
                                        </td>
                                        <td>
                                            <%= cliente.cedulaPropietario %>
                                        </td>

                                        <!-- <td>
                                            <a href="#" class="btn btn-outline-info"><i class='bx bxs-edit-alt'></i></a>
                                            <a href="#" class="btn btn-outline-danger"><i class='bx bxs-trash'></i></a>
                                        </td> -->
                                    </tr>
                                    <% })%>

                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </main>
    <footer>

    </footer>

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM"
        crossorigin="anonymous"></script>

    <script src="https://code.jquery.com/jquery-3.5.1.js"></script>
    <script src="https://cdn.datatables.net/1.10.25/js/jquery.dataTables.min.js"></script>
    <script src="https://cdn.datatables.net/1.10.25/js/dataTables.bootstrap5.min.js"></script>
    <script>
        $(document).ready(function () {
            $('#vehiculo').DataTable();

        });
    </script>
</body>

</html>